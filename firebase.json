{
  "hosting": {
    "site": "oimonotes-node",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "public": "dist",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "oimonotes-node",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
